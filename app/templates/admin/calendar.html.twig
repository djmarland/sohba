{% extends "admin/base.html.twig" %}

{% block body %}
    <h1>Calendar</h1>
    <p class="unit">
        Highlighted Days have changes to the regular schedule. Click a day to make changes.
    </p>
    <div class="message message--warning">
        If you still need to use the old calendar it can temporarily still be used
        <a href="/admin-old/index.php">here</a>. Please let me know why though so I can fix the new one.
    </div>
    <div id="app" data-container="EditCalendar"></div>
{% endblock %}

{% block footScripts %}
    <script>
      window.HBAContent = {{ pageData | raw }};
    </script>
{% endblock %}
